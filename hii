public static void main(String[] args) {
		String a = "abcd@gmail.com";
		//o/p = a***@*******m
		System.out.println(a.charAt(0));
		
		for(int i =1;i<=a.length()-2;i++) {
			if(a.charAt(i)=='@') {
				System.out.println("@");
			}
			else {
				System.out.println("*");
			}
			
		}
		System.out.println(a.charAt(a.length()-1));
		